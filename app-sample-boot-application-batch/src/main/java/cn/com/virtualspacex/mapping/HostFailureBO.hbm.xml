<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.com.fujielectric.model">
    <class name="cn.com.fujielectric.model.bo.HostFailureBO" table="onlinesaleinfos">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="locationCode" column="location_code"/>
        <property name="vmCode" column="vm_code"/>
        <property name="posCode" column="pos_code"/>
        <property name="saleSerialEndId" column="saleSerialEndId"/>
        <property name="failureValue" column="failureValue"/>
        <property name="systemTime" column="system_time"/>
    </class>
</hibernate-mapping>