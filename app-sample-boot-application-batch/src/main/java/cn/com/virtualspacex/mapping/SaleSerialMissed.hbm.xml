<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.com.fujielectric.model">
    <class name="cn.com.fujielectric.model.SaleSerialMissed" table="sale_serial_missed" dynamic-insert="true">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="locationCode" column="location_code"/>
        <property name="vmCode" column="vm_code"/>
        <property name="posCode" column="pos_code"/>
        <property name="saleSerial" column="sale_serial"/>
        <property name="retryTimes" column="retry_times"/>
        <property name="isStatus" column="is_status"/>
        <property name="isDeleted" column="is_deleted"/>
        <property name="createdAt" column="created_at"/>
        <property name="updatedAt" column="updated_at"/>
    </class>
</hibernate-mapping>