<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.com.fujielectric.model">
    <class name="cn.com.fujielectric.model.SaleSerialEnd" table="sale_serial_end" dynamic-update="true" >
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="locationCode" column="location_code" />
        <property name="vmCode" column="vm_code" />
        <property name="posCode" column="pos_code" />
        <property name="saleSerialEnd" column="sale_serial_end"/>
        <property name="failureNum" column="failure_num"/>
        <property name="checkedAt" column="checked_at"/>
        <property name="createdAt" column="created_at"/>
        <property name="updatedAt" column="updated_at"/>
    </class>
</hibernate-mapping>